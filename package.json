{
  "private": true,
  "name": "iridoscope",
  "type": "module",
  "description": "Basic iris diagnosis",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "node --experimental-json-modules node_modules/webpack/bin/webpack serve --port 1234 --hot --history-api-fallback --module-strict-export-presence --stats-errors --stats-error-details true --mode development --config webpack/webpack.config.js",
    "build": "npm-run-all build:clean build:create",
    "build:create": "npm-run-all build:bundle build:copy-readme build:move-index-html",
    "build:clean": "rimraf build",
    "build:copy-readme": "cpy readme.build.txt build --flat --rename=readme.txt",
    "build:move-index-html": "npm-run-all build:copy-index-html build:delete-index-html",
    "build:copy-index-html": "cpy build/assets/index.html build --flat",
    "build:delete-index-html": "rimraf build/assets/index.html",
    "build:bundle": "node --experimental-json-modules node_modules/webpack/bin/webpack --mode production --progress --module-strict-export-presence --stats-errors --stats-error-details true --color --config ./webpack/webpack.config.prod.js",
    "create-404-page": "cpy build/index.html build --flat --rename=404.html",
    "copy-website-files": "cpy website/** build",
    "test": "node --experimental-json-modules node_modules/mocha/bin/_mocha --require ./test/setup.js --reporter spec --colors --bail --exit --recursive \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "@sentry/browser": "^7.49.0",
    "classnames": "^2.3.2",
    "core-js": "^3.30.1",
    "easy-react-form": "^2.0.8",
    "file-saver": "^2.0.5",
    "flexible-json-schema": "^0.12.5",
    "intl-messageformat": "^10.3.4",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pages": "^0.7.2",
    "react-redux": "^8.0.5",
    "react-responsive-ui": "^0.15.45",
    "redux": "^4.2.0",
    "request-animation-frame-timeout": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^7.0.0",
    "@swc/core": "^1.3.53",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-calc": "^8.2.4",
    "postcss-custom-properties": "^13.1.5",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.2.4",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "react-refresh": "^0.14.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "universal-webpack": "^0.8.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/iridoscope/iridoscope.git"
  },
  "keywords": [
    "iris",
    "analysis"
  ],
  "author": "@iridoscope",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/iridoscope/iridoscope/issues"
  },
  "homepage": "https://gitlab.com/iridoscope/iridoscope#readme"
}
